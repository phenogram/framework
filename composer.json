{
    "name": "shanginn/telegram-bot-api-framework",
    "description": "Telegram bot API framework",
    "type": "library",
    "version": "0.20.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shanginn\\TelegramBotApiFramework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shanginn\\TelegramBotApiFramework\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Nikolai Shangin",
            "email": "shanginn@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "shanginn/telegram-bot-api-bindings": "dev-master",
        "psr/log": "^3.0",
        "psr-discovery/log-implementations": "^1.0",
        "psr/http-factory": "^1.0",
        "react/event-loop": "^1.4",
        "react/promise": "^3",
        "react/async": "^4.1",
        "react/promise-timer": "^1.10",
        "react/http": "^1.9",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3",
        "vlucas/phpdotenv": "^5.5",
        "monolog/monolog": "^3.4",
        "phpunit/phpunit": "^11"
    },
    "minimum-stability": "stable",
    "scripts": {
        "fix": [
            "PHP_CS_FIXER_IGNORE_ENV=1 ./tools/php-cs-fixer/vendor/bin/php-cs-fixer fix -vvv --config=.php-cs-fixer.php --allow-risky=yes"
        ]
    }
}
